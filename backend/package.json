{
  "name": "legal-estate-backend",
  "version": "1.0.0",
  "description": "Legal Estate Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:verbose": "jest --verbose --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=integration --runInBand",
    "test:integration": "jest --testPathPattern=api --runInBand",
    "test:performance": "jest --testPathPattern=performance --verbose --runInBand",
    "test:error-handling": "jest --testPathPattern=error-handling --runInBand",
    "test:ci": "jest --coverage --ci --watchAll=false --runInBand",
    "test:setup": "node ../scripts/test-setup.js",
    "test:env": "node ../scripts/test-env.js",
    "test:dependencies": "node ../scripts/test-dependencies.js",
    "test:db-connection": "node ../scripts/test-db-connection.js",
    "test:all": "npm run test:setup && npm run test:env && npm run test:dependencies && npm run test:db-connection && npm run test:coverage"
  },
